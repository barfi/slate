        </span><span class="s2">"url"</span><span class="p">:</span><span class="w"> </span><span class="s2">"/api/stock/issuer/e234a68c-ddcf-478f-8019-25c1abdff4ca/"</span><span class="w">
    </span><span class="p">},</span><span class="w">
    </span><span class="s2">"potential"</span><span class="p">:</span><span class="w"> </span><span class="p">{},</span><span class="w">
    </span><span class="s2">"price"</span><span class="p">:</span><span class="w"> </span><span class="p">{},</span><span class="w">
    </span><span class="s2">"uuid"</span><span class="p">:</span><span class="w"> </span><span class="s2">"c0b0e353-a9fe-4567-8b63-c6c29b133359"</span><span class="p">,</span><span class="w">
    </span><span class="s2">"name"</span><span class="p">:</span><span class="w"> </span><span class="s2">""</span><span class="p">,</span><span class="w">
